{
  "name": "sooviet/flickrmini",

  "description": "Flickr Mini Gallery App",

  "require": {
    "php": ">=7"
  },

  "require-dev": {
    "vlucas/phpdotenv": "^3.4"
  },

  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/helper.php"
    ],
    "classmap": [
      "app/Helpers"
    ]
  },


  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },

  "minimum-stability": "dev",

  "prefer-stable": true
}
