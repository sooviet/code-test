{
  "name": "sooviet/flickrmini",

  "description": "Flickr Mini App",

  "require": {
    "php": ">=7"
  },

  "require-dev": {
  },

  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/helper.php"
    ]
  },


  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },

  "minimum-stability": "dev",

  "prefer-stable": true
}
